FUNCTION	fact	:
PARAM	n
IF	n	==	#1	GOTO	label1
GOTO	label2
LABEL	label1	:
RETURN	n

GOTO	label3
LABEL	label2	:
t1	:=	n	-	#1
ARG	t1
t2	:=	CALL	fact
t3	:=	n	+	t2
RETURN	t3

GOTO	label3
LABEL	label3	:
FUNCTION	main	:
READ	t4
m	:=	t4
IF	m	>	#1	GOTO	label4
GOTO	label5
LABEL	label4	:
ARG	m
t5	:=	CALL	fact
result	:=	t5
GOTO	label6
LABEL	label5	:
result	:=	#1
GOTO	label6
LABEL	label6	:
t6	:=	result
WRITE	t6
RETURN	#0

