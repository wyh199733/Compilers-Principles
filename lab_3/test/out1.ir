FUNCTION	main:
READ	t1
n	:=	t1
IF	n>#0	GOTO	label1
GOTO	label2
LABEL	label1	:
t2	:=	#1
WRITE	t2
GOTO	label3
LABEL	label2	:
IF	n<#0	GOTO	label4
GOTO	label5
LABEL	label4	:
t4	:=	#0	-	#1
t3	:=	t4
WRITE	t3
GOTO	label6
LABEL	label5	:
t5	:=	#0
WRITE	t5
GOTO	label6
LABEL	label6	:
GOTO	label3
LABEL	label3	:
RETURN	#0

